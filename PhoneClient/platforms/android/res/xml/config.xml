<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Accelerometer">
        <param name="android-package" value="org.apache.cordova.devicemotion.AccelListener" />
    </feature>
    <feature name="Compass">
        <param name="android-package" value="org.apache.cordova.deviceorientation.CompassListener" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="CameraPlus">
        <param name="android-package" value="com.moonware.cameraplus.CameraPlus" />
    </feature>
    <feature name="CameraServer">
        <param name="android-package" value="com.moonware.cameraserver.CameraServer" />
    </feature>
    <feature name="CanvasCamera">
        <param name="android-package" value="com.keith.canvascameraplugin.CanvasCamera" />
    </feature>
    <feature name="VolumeButtons">
        <param name="android-package" value="com.manueldeveloper.VolumeButtonsListener" />
    </feature>
    <allow-intent href="market:*" />
    <name>TeaBeeD</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <preference name="phonegap-version" value="3.1.0" />
    <preference name="fullscreen" value="true" />
    <preference name="orientation" value="portrait" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <gap:plugin name="com.keith.cordova.plugin.canvascamera" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
